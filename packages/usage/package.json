{
  "name": "@mdcode-ts/usage",
  "version": "0.0.1",
  "description": "End-to-end tests for @mdcode-ts/mdcode",
  "type": "module",
  "private": true,
  "scripts": {
    "test": "vitest",
    "test:watch": "vitest --watch",
    "lint:ts": "tsc --noEmit",
    "lint": "tsc --noEmit ; eslint \"{tests,fixtures}/**/*.{j,t}s{,x}\" --cache --max-warnings=0",
    "lint:s": "eslint --config .eslintrc.style.json \"{tests,fixtures}/**/*.{j,t}s{,x}\" --cache --max-warnings=0",
    "lint:fix": "eslint --config .eslintrc.style.json \"{tests,fixtures}/**/*.{j,t}s{,x}\" --cache --max-warnings=0 --fix"
  },
  "keywords": [
    "mdcode-ts",
    "mdcode",
    "test",
    "e2e"
  ],
  "author": "Adrian Elton-Browning",
  "license": "ISC",
  "engines": {
    "node": ">=22.0.0"
  },
  "devDependencies": {
    "@types/node": "^24.9.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.2"
  },
  "dependencies": {
    "@mdcode-ts/mdcode": "workspace:*"
  }
}
