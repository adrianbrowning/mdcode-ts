{
  "name": "usage",
  "version": "0.0.1",
  "description": "Examples and end-to-end tests for mdcode",
  "type": "module",
  "private": true,
  "scripts": {
    "test": "node --test-reporter=spec --test tests/**/*.test.ts",
    "test:watch": "node --test-reporter=spec --test --watch tests/**/*.test.ts",
    "lint:ts": "tsc --noEmit",
    "lint": "tsc --noEmit ; eslint \"{tests,fixtures,examples}/**/*.{j,t}s{,x}\" --cache --max-warnings=0",
    "lint:s": "eslint --config .eslintrc.style.json \"{tests,fixtures,examples}/**/*.{j,t}s{,x}\" --cache --max-warnings=0",
    "lint:fix": "eslint --config .eslintrc.style.json \"{tests,fixtures,examples}/**/*.{j,t}s{,x}\" --cache --max-warnings=0 --fix",
    "example:extract": "mdcode extract examples/sample.md",
    "example:list": "mdcode list examples/sample.md",
    "example:update": "mdcode update examples/sample.md",
    "example:transform": "mdcode update --transform examples/transforms/uppercase-sql.ts examples/test-transform.md"
  },
  "keywords": [
    "mdcode",
    "mdcode",
    "test",
    "e2e"
  ],
  "author": "Adrian Elton-Browning",
  "license": "ISC",
  "engines": {
    "node": ">=22.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.18.12",
    "typescript": "^5.9.3"
  },
  "dependencies": {
   "mdcode": "workspace:mdcode@*"
  }
}
